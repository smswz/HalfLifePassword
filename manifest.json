{
	"name": "Password Half-Life",
	"version": "0.1",
	"description": "An extension to substitute temporary passwords for the real password.",
	"browser_action": {
		"default_icon": "icon_48.png"
	},
	"permissions": [
		"http://*/*",
		"https://*/*"
	],
	"icons": {
		"16": "icon_16.png",
		"32": "icon_48.png",
		"128": "icon_128.png"
	},
	"content_scripts": [
		{ "matches": ["http://*/*", "https://*/*"], "js": ["context.js"] }
	]
}
